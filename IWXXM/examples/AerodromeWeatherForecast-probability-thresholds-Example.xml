<?xml version="1.0" encoding="UTF-8"?>
<!--
    A sample file for Aerodrome Weather Forecast Objects

-->
<iwxxm:MeteorologicalFeatureCollection 
    xmlns:iwxxm="http://icao.int/iwxxm/sx"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xmlns:aixm="http://www.aixm.aero/schema/5.1.1"
    xmlns:tsml="http://www.opengis.net/timeseriesml/1.3/iwxxm"
    xmlns:swe="http://www.opengis.net/swe/2.0"
    xmlns:cis="http://www.opengis.net/cis/1.1/gml"
    xmlns:gco="http://www.isotc211.org/2005/gco"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.aixm.aero/schema/5.1.1 http://www.aixm.aero/schema/5.1.1/AIXM_Features.xsd
    http://icao.int/iwxxm/sx ../iwxxm.xsd"
    gml:id="uuid.ce3b61b9-d360-4da1-83b9-67e668406869" reportStatus="NORMAL" permissibleUsage="OPERATIONAL">

    <!-- Unique identifier of the collective -->
    <gml:identifier codeSpace="http://wafs/sigwxfc">3ae2138f-12d8-4392-b4d7-7a2d2e2e55d8</gml:identifier>
    
    <!-- A temporal bounding box containing the phenomenonTime of all objects -->
    <iwxxm:boundingPeriod>
        <gml:TimePeriod gml:id="uuid.76096601-6d6c-41f8-ad23-a25b1a8f3658">
            <gml:beginPosition>2001-01-31T00:00:00Z</gml:beginPosition>
            <gml:endPosition>2001-01-31T00:50:00Z</gml:endPosition>
        </gml:TimePeriod>
    </iwxxm:boundingPeriod>
    
    <!-- A spatial bounding box containing the phenomenonGeometry of all objects -->
    <iwxxm:boundingVolume>
        <iwxxm:ElevatedEnvelope>
            <gml:lowerCorner srsDimension="2" axisLabels="Lat Long" srsName="http://www.opengis.net/def/crs/EPSG/0/4326">-90 -180</gml:lowerCorner>
            <gml:upperCorner srsDimension="2" axisLabels="Lat Long" srsName="http://www.opengis.net/def/crs/EPSG/0/4326">90 180</gml:upperCorner>
            <iwxxm:upperElevation uom="FL">600</iwxxm:upperElevation>
            <iwxxm:upperVerticalReference>STD</iwxxm:upperVerticalReference>
            <iwxxm:lowerElevation uom="FL">100</iwxxm:lowerElevation>
            <iwxxm:lowerVerticalReference>STD</iwxxm:lowerVerticalReference>
        </iwxxm:ElevatedEnvelope>
    </iwxxm:boundingVolume>
    
    <!-- A list of all phenomena of all objects -->
    <iwxxm:phenomenaList xlink:href="http://codes.wmo.int/49-2/MeteorologicalFeature/PHENOMENON"/>
    
    <!-- Issue time of the collective -->
    <iwxxm:issueTime>
        <gml:TimeInstant gml:id="uuid.4b5156bd-32e1-4fc9-a8f0-8f6b6c96e544">
            <gml:timePosition>2001-01-30T21:00:00Z</gml:timePosition>
        </gml:TimeInstant>
    </iwxxm:issueTime>
    
    <!-- Originating centre of the collective -->
    <iwxxm:originatingCentre>
        <iwxxm:WorldAreaForecastCentre>London</iwxxm:WorldAreaForecastCentre>
    </iwxxm:originatingCentre>
    
    <!-- Category of objects, either weatherObservations or weatherForecasts -->
    <iwxxm:phenomenonCategory>weatherForecasts</iwxxm:phenomenonCategory>
    
    <!-- If the category is weatherForecasts the forecast base time (if available) -->
    <iwxxm:phenomenonBaseTime>
        <gml:TimeInstant gml:id="uuid.825a0f0f-0b48-4a0b-9270-7f1075fed151">
            <gml:timePosition>2001-01-30T18:00:00Z</gml:timePosition>
        </gml:TimeInstant>
    </iwxxm:phenomenonBaseTime>
    
    <!-- phenomenonTime is the time when the collective is valid -->
    <iwxxm:phenomenonTime>
        <gml:TimePeriod gml:id="uuid.26328dbd-c763-4fd4-9b3c-40a6400795f0">
            <gml:beginPosition>2001-01-31T00:00:00Z</gml:beginPosition>
            <gml:endPosition>2001-01-31T00:50:00Z</gml:endPosition>
        </gml:TimePeriod>
    </iwxxm:phenomenonTime>
    
    <!-- A wind weather forecast object with a timeseries of probabilities of falling between or exceeding given quantity values  -->
    <iwxxm:feature>
        <iwxxm:AerodromeWeatherPhenomenonForecast gml:id="uuid.2106d31a-7406-4d3a-aff9-c522ae0ad73e">
            <gml:identifier codeSpace="http://wafs/sigwxobj">wxobj1</gml:identifier>
            <iwxxm:phenomenonTime>
                <gml:TimePeriod gml:id="uuid.1bd80823-7dd5-4e15-a74c-f0238189e22a">
                    <gml:beginPosition>2001-01-31T00:00:00Z</gml:beginPosition>
                    <gml:endPosition>2001-01-31T00:50:00Z</gml:endPosition>
                </gml:TimePeriod>
            </iwxxm:phenomenonTime>
            
            <iwxxm:phenomenon xlink:href="http://preferred.observable-property.registry/wind"/>
            
            <iwxxm:phenomenonGeometry>
                <gml:Polygon gml:id="uuid.e2442166-751c-40cc-83ae-836025b6ccda">
                    <gml:exterior>
                        <gml:LinearRing>
                            <gml:posList></gml:posList>
                        </gml:LinearRing>
                    </gml:exterior>
                </gml:Polygon>
            </iwxxm:phenomenonGeometry>
          
            <iwxxm:phenomenonProperty>
                <iwxxm:WeatherPhenomenonTimeseriesTVP gml:id="uuid.64e1ff47-0287-4dbd-85d4-b7a7eccc9d44">
                    
                    <tsml:metadata>
                        <tsml:TimeseriesMetadata>
                            <tsml:numberTimeSteps>6</tsml:numberTimeSteps>
                        </tsml:TimeseriesMetadata>
                    </tsml:metadata>
                    
                    <tsml:point>
                        <tsml:RecordTVP>
                            <tsml:time>2001-01-31T00:00:00Z</tsml:time>
                            <tsml:value>0.84 0.95 0.99</tsml:value>
                        </tsml:RecordTVP>
                    </tsml:point>
                    <tsml:point>
                        <tsml:RecordTVP>
                            <tsml:time>2001-01-31T00:10:00Z</tsml:time>
                            <tsml:value>0.90 0.94 0.98</tsml:value>
                        </tsml:RecordTVP>
                    </tsml:point>
                    <tsml:point>
                        <tsml:RecordTVP>
                            <tsml:time>2001-01-31T00:20:00Z</tsml:time>
                            <tsml:value>0.82 0.94 0.97</tsml:value>
                        </tsml:RecordTVP>
                    </tsml:point>
                    <tsml:point>
                        <tsml:RecordTVP>
                            <tsml:time>2001-01-31T00:30:00Z</tsml:time>
                            <tsml:value>0.89 0.92 0.95</tsml:value>
                        </tsml:RecordTVP>
                    </tsml:point>
                    <tsml:point>
                        <tsml:RecordTVP>
                            <tsml:time>2001-01-31T00:40:00Z</tsml:time>
                            <tsml:value>0.93 0.90 0.96</tsml:value>
                        </tsml:RecordTVP>
                    </tsml:point>
                    <tsml:point>
                        <tsml:RecordTVP>
                            <tsml:time>2001-01-31T00:50:00Z</tsml:time>
                            <tsml:value>0.90 0.96 0.98</tsml:value>
                        </tsml:RecordTVP>
                    </tsml:point>
                    <iwxxm:pointRecord>
                        <swe:DataRecord>
                            
                            <swe:field name="windSpeedExceedenceProbability">
                                <iwxxm:QuantityValueExceedenceProbability>
                                    <swe:label>Probability of 10 min average wind speed exceeding 15.0 m/s</swe:label>
                                    <swe:uom xlink:href="https://qudt.org/vocab/unit/UNITLESS" />
                                    <iwxxm:limit>
                                        <swe:Quantity definition="http://preferred.observable-property.registry/windSpeed">
                                            <swe:label>Wind speed</swe:label>
                                            <swe:uom xlink:href="https://unitsofmeasure.org/ucum#m_s" code="m/s"/>
                                            <swe:value>15.0</swe:value>
                                        </swe:Quantity>
                                    </iwxxm:limit>
                                    <iwxxm:limitQuantityMetadata>
                                        <tsml:PointMetadata>
                                            <tsml:interpolationType xlink:href="http://www.opengis.net/def/timeseries/InterpolationCode/AveragePrec"/>
                                            <tsml:aggregationDuration>PT10M</tsml:aggregationDuration>
                                        </tsml:PointMetadata>
                                    </iwxxm:limitQuantityMetadata>
                                </iwxxm:QuantityValueExceedenceProbability>
                            </swe:field>
                            
                            <swe:field name="windDirectionProbability">
                                <iwxxm:QuantityValueBetweenProbability>
                                    <swe:label>Probability of 10 min average wind direction falling between 90 and 110 degrees</swe:label>
                                    <swe:uom xlink:href="https://qudt.org/vocab/unit/UNITLESS" />
                                    <iwxxm:range>
                                        <swe:QuantityRange>
                                            <swe:label>Wind direction</swe:label>
                                            <swe:uom xlink:href="https://unitsofmeasure.org/ucum#deg" code="deg"/>
                                            <swe:value>90 110</swe:value>
                                        </swe:QuantityRange>
                                    </iwxxm:range>
                                    <iwxxm:rangeQuantityMetadata>
                                        <tsml:PointMetadata>
                                            <tsml:interpolationType xlink:href="http://www.opengis.net/def/timeseries/InterpolationCode/AveragePrec"/>
                                            <tsml:aggregationDuration>PT10M</tsml:aggregationDuration>
                                        </tsml:PointMetadata>
                                    </iwxxm:rangeQuantityMetadata>
                                </iwxxm:QuantityValueBetweenProbability>
                            </swe:field>
                            
                            <swe:field name="windGustExceedenceProbability">
                                <iwxxm:QuantityValueExceedenceProbability>
                                    <swe:label>Probability of 10 min maximun wind gust exceeding 20.0 m/s</swe:label>
                                    <swe:uom xlink:href="https://qudt.org/vocab/unit/UNITLESS" />
                                    <iwxxm:limit>
                                        <swe:Quantity definition="http://preferred.observable-property.registry/windGust">
                                            <swe:label>Wind gust</swe:label>
                                            <swe:uom xlink:href="https://unitsofmeasure.org/ucum#m_s" code="m/s"/>
                                            <swe:value>20.0</swe:value>
                                        </swe:Quantity>
                                    </iwxxm:limit>
                                    <iwxxm:limitQuantityMetadata>
                                        <tsml:PointMetadata>
                                            <tsml:interpolationType xlink:href="http://www.opengis.net/def/timeseries/InterpolationCode/MaxPrec"/>
                                            <tsml:aggregationDuration>PT10M</tsml:aggregationDuration>
                                        </tsml:PointMetadata>
                                    </iwxxm:limitQuantityMetadata>
                                </iwxxm:QuantityValueExceedenceProbability>
                            </swe:field>
                            
                        </swe:DataRecord>
                    </iwxxm:pointRecord>
                </iwxxm:WeatherPhenomenonTimeseriesTVP>
            </iwxxm:phenomenonProperty>
            <iwxxm:aerodrome>
                <aixm:AirportHeliport gml:id="uuid.522db66a-3bfb-4206-8292-cd453bc77aba">
                    <aixm:timeSlice>
                        <aixm:AirportHeliportTimeSlice gml:id="uuid.bec75672-e08b-49aa-a662-ffc8bab9e39b">
                            <gml:validTime></gml:validTime>
                            <aixm:interpretation>SNAPSHOT</aixm:interpretation>
                            <aixm:ARP>
                                <aixm:ElevatedPoint gml:id="uuid.d7498f1a-8a1e-4130-9781-4febd3588a75" srsDimension="2" axisLabels="Lat Long" srsName="http://www.opengis.net/def/crs/EPSG/0/4326">
                                    <gml:pos>12.34 -12.34</gml:pos>
                                    <aixm:elevation uom="M">12</aixm:elevation>
                                    <aixm:verticalDatum>EGM_96</aixm:verticalDatum>
                                </aixm:ElevatedPoint>
                            </aixm:ARP>
                        </aixm:AirportHeliportTimeSlice>
                    </aixm:timeSlice>
                </aixm:AirportHeliport>
            </iwxxm:aerodrome>
        </iwxxm:AerodromeWeatherPhenomenonForecast>
    </iwxxm:feature>
</iwxxm:MeteorologicalFeatureCollection>
